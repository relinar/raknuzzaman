class animal:
    def __init__(self,name,alive):
        self.name=name
        self.alive=True
    def eat(self):
        print(f"{self.name} is eating")
    def sleep(self):
        print(f"{self.name} is sleeping")


class dog:
    def __init__(self,name,alive):
        self.name=name
        self.alive=True
    def eat(self):
        print(f"{self.name} is eating")
    def sleep(self):
        print(f"{self.name} is sleeping")


class cat:
    def __init__(self,name,alive):
        self.name=name
        self.alive=True
    def eat(self):
        print(f"{self.name} is eating")
    def sleep(self):
        print(f"{self.name} is sleeping")

class dog(animal):
    def character(self):
        print(f"{self.name} is faithful")

class cat(animal):
    def look(self):
        print(f"{self.name} is beautiful to look at")


dog1=dog("Tomi", True)
dog1.eat()

cat1=cat("Gigi", True)
cat1.eat()
cat1.look()
